<div class="panel breadcrumb" style="background-color: #ffffff">
    <li><a href="#">Syllabus</a></li>
    <li><a href="#">Course</a></li>
    <li><a href="#">Sem</a></li>
    <li><a href="#">Subject</a></li>
    <li><a href="#">Create Subject</a></li>
    <div class="panel-body sy-menu text-center  " style="color:#ffffff;font-size: 20px;">CRETAE BRANCH</div>
    <div class="panel-body">
        <div class="row">
            <div class="col-sm-8 col-md-6">
                <form id="uploadForm" enctype="multipart/form-data" action="/syllabus/course/sem/branch/addsubj/{{brName}}" method="post" name="uploadForm">
                    <div class="form-group">
                        <label for="inputsm">Course Name</label>
                        <input class="form-control input-sm" value="{{courseName}}" id="inputcrs" name="inputcrs"
                               type="text" disabled>
                    </div>
                    <div class="form-group">
                        <label for="inputsm">Semester Name</label>
                        <input class="form-control input-sm" value="{{semName}}" id="inputsm" name="inputsm" type="text"
                               disabled>
                    </div>
                    <div class="form-group">
                        <label for="inputsm">Branch Name</label>
                        <input class="form-control input-sm" value="{{brName}}" id="inputbr" name="inputsm" type="text"
                               disabled>
                    </div>
                    <div class="form-group">
                        <label for="inputsm">Subject Code</label>
                        <input class="form-control input-sm" id="inputsubcode" name="inputsubcode" type="text"
                               placeholder="Subject Code" required>
                    </div>
                    <div class="form-group">
                        <label for="inputsm">Subject Name</label>
                        <input class="form-control input-sm" id="inputsub" name="inputsub" type="text"
                               placeholder="Subject Name" required>
                    </div>

                    <div class="fileinput fileinput-new" id="formInfo">
                        <input name="myfile" type="file"/>
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Upload" name="submit">
                    </div>
                    <div class="form-group">
                        <span id="status"></span>
                    </div>

                </form>

            </div>
        </div>
    </div>
</div>
<script src="http://cdnjs.cloudflare.com/ajax/libs/jquery.form/3.51/jquery.form.min.js"></script>
<script>
    $(document).ready(function() {

        $('#uploadForm').submit(function() {
            $("#status").empty().text("File is uploading...");
            $(this).ajaxSubmit({

                error: function(xhr) {
                    status('Error: ' + xhr.status);
                },

                success: function(response) {
                    $("#status").empty().text(response);

                    console.log(response);
                }
            });
            //Very important line, it disable the page refresh.
            return false;
        });
    });
</script>

<script src="/js/sidepanel_home.js"></script>